{
    "name": "DeRaja/hashtagcms",
    "description": "Headless CMS, Use it as Headless CMS or Bundled, API enabled, Admin Panel, Multisite, Multitenant, Multilingual, and oAuth 2.0.\nThe Most powerful, fast and user-friendly platform. Made with PHP Laravel Framework.",
    "license": "MIT",
    "authors": [
        {
            "name": "Marghoob Suleman",
            "email": "marghoobsuleman@gmail.com",
            "homepage": "https://www.marghoobsuleman.com"
        }
    ],
    "homepage": "https://github.com/DeRaja/hashtagcms",
    "keywords": ["Laravel", "HashtagCms", "Marghoob Suleman", "Headless CMS", "CMS", "Mobile Api Supported CMS", "Hashtag Cms"],
    "require": {
        "laravel/ui": "^3.0",
        "laravel/passport": "^10.0",
        "laravel/socialite": "^5.0",
        "usmanhalalit/laracsv": "^2.1",
        "intervention/image": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MarghoobSuleman\\HashtagCms\\": "src/"
        },
        "files": [
            "src/Core/Helpers/AdminHelper.php",
            "src/Core/Helpers/FormHelper.php",
            "src/Core/Helpers/FrontendHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MarghoobSuleman\\HashtagCms\\Tests\\": "tests"
        }
    },
    "repositories": {
        "foo": {
            "type": "composer",
            "url": "https://github.com/DeRaja/hashtagcms"
        }
    },
   "dependencies": {
            "follow-redirects": ">=1.14.7"
    },
    "extra": {
        "laravel": {
            "aliases": {
                "HashtagCms": "MarghoobSuleman\\HashtagCms\\Facades\\HashtagCms"
            }
        }
    }
}
