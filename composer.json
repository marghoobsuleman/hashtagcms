{
    "name": "marghoobsuleman/hashtagcms",
    "description": "Headless CMS, Use it as Headless CMS or Bundled, API enabled, Admin Panel, Multisite, Multi-platform, Multilingual.\nThe Most powerful, fast and user-friendly platform. Made with PHP Laravel Framework.",
    "license": "MIT",
    "authors": [
        {
            "name": "Marghoob Suleman",
            "email": "marghoobsuleman@gmail.com",
            "homepage": "https://www.marghoobsuleman.com"
        }
    ],
    "homepage": "https://github.com/marghoobsuleman/hashtagcms",
    "keywords": ["Laravel", "HashtagCms", "Marghoob Suleman", "Headless CMS", "CMS", "Mobile Api Supported CMS", "Hashtag Cms"],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "usmanhalalit/laracsv": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.4.4",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MarghoobSuleman\\HashtagCms\\": "src/"
        },
        "files": [
            "src/Core/Helpers/AdminHelper.php",
            "src/Core/Helpers/FormHelper.php",
            "src/Core/Helpers/FrontendHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MarghoobSuleman\\HashtagCms\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarghoobSuleman\\HashtagCms\\HashtagCmsServiceProvider"
            ],
            "aliases": {
                "HashtagCms": "MarghoobSuleman\\HashtagCms\\Facades\\HashtagCms"
            }
        }
    }
}